    },
    "breve": {
      "type": "text"
    },
    "contenuto": {
      "type": "text"
    },
    "anno": {
      "type": "integer"
    },
    "mese": {
      "type": "integer"
    },
    "lingua": {
      "type": "enumeration",
      "enum": [
        "Italiano",
        "Inglese",
        "Arabo"
      ],
      "required": true,
      "default": "Italiano"
    },
    "autore": {
      "type": "text"
    },
    "citt√†": {
      "type": "text"
    },
    "serie": {
      "type": "text"
    },
    "formato": {
      "type": "text"
    },
    "isbn": {
      "type": "text"
    },
    "commento": {
      "type": "text"
    },
    "file": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "contano-le-pagine": {
      "type": "integer"
    },
    "intestazione": {
      "type": "text"
    },
    "acquistare": {
      "type": "text"
    },
